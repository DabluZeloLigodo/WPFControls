<UserControl x:Class="MefExample.PluginOne.PluginExampleControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MefExample.PluginOne"
             mc:Ignorable="d" 
              d:DesignHeight="300" d:DesignWidth="300">
    <Grid Background="#bf0A10">
        <Label Foreground="White" Content="{Binding PluginHeadLine}" HorizontalAlignment="Center" VerticalAlignment="Center"></Label>
    </Grid>
</UserControl>
